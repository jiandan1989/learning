    "deploy": "gh-pages -d dist",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "chalk": "^2.4.1",
    "classnames": "^2.2.5",
    "element-ui": "^2.3.9",
    "gh-pages": "^1.1.0",
    "js-cookie": "^2.2.0",
    "koa": "^2.5.1",
    "koa-bodyparser": "^4.2.1",
    "koa-convert": "^1.2.0",
    "koa-router": "^7.4.0",
    "mongoose": "^5.1.4",
    "nodemon": "^1.17.5",
    "vue": "^2.5.16",
    "vue-i18n": "^7.7.0",
    "vue-particles": "^1.0.9",
    "vue-resource": "^1.5.1",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-beta.11",
    "@vue/cli-plugin-eslint": "^3.0.0-beta.11",
    "@vue/cli-service": "^3.0.0-beta.11",
    "@vue/eslint-config-airbnb": "^3.0.0-beta.11",
    "babel-cli": "^6.26.0",
    "babel-plugin-component": "^0.11.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.1",
    "ts-node": "^6.1.1",
    "vue-template-compiler": "^2.5.16"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "plugin:vue/essential",
      "@vue/airbnb"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
